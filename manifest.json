{
  "manifest_version": 3,
  "name": "YWH Scopes Extractor",
  "version": "1.0",
  "description": "Extract scopes from yeswehack",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "ywh-scopes-extractor@wlayzz.addon",
      "strict_min_version": "109.0"
    }
  }
}